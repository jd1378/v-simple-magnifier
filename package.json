{
  "name": "v-simple-magnifier",
  "version": "1.0.3",
  "description": "a vue directive to use on img elements to zoom on hover",
  "author": "jd1378",
  "license": "MIT",
  "main": "dist/index.ssr.js",
  "browser": "dist/index.esm.js",
  "module": "dist/index.esm.js",
  "unpkg": "dist/index.min.js",
  "files": [
    "dist/*",
    "src/**/*.vue",
    "!src/serve-dev.*"
  ],
  "keywords": [
    "vue",
    "directive",
    "magnifier",
    "zoom"
  ],
  "scripts": {
    "serve": "vue-cli-service serve src/serve-dev.js",
    "build": "cross-env NODE_ENV=production rollup --config build/rollup.config.js",
    "build:ssr": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format cjs",
    "build:es": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format es",
    "build:unpkg": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format iife"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:jd1378/v-simple-magnifier.git"
  },
  "dependencies": {
    "node-sass": "^4.13.1",
    "sass-loader": "^8.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.7.7",
    "@babel/preset-env": "^7.7.7",
    "@rollup/plugin-alias": "^2.2.0",
    "@rollup/plugin-commonjs": "^11.0.1",
    "@rollup/plugin-replace": "^2.2.1",
    "@vue/cli-plugin-babel": "^4.1.0",
    "@vue/cli-service": "^4.1.0",
    "babel-eslint": "^10.1.0",
    "cross-env": "^6.0.3",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-loader": "^4.0.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-vue": "^6.2.2",
    "minimist": "^1.2.0",
    "prettier": "^2.0.4",
    "rollup": "^1.27.13",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-terser": "^5.1.3",
    "rollup-plugin-vue": "^5.1.5",
    "vue": "^2.6.10",
    "vue-template-compiler": "^2.6.10"
  },
  "peerDependencies": {
    "vue": "^2.6.10"
  },
  "engines": {
    "node": ">=8"
  }
}
